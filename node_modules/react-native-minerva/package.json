{
  "name": "react-native-minerva",
  "title": "React Native Minerva",
  "version": "7.1.1",
  "description": "UI components Banco de Chile",
  "main": "./src/index.js",
  "scripts": {
    "crear-plantilla": "node scripts/generate/index",
    "crear-componente": "sh scripts/template.sh",
    "release": "standard-version",
    "test": "jest --config=jest/jest.config.js --detectOpenHandles",
    "test:watch": "jest --watch --verbose false",
    "sonar:scan": "node sonarScanner",
    "sonar": "npx eslint src -o ./eslint-report.json --format json; npx jest  --detectOpenHandles --config=jest/jest.config.js; node sonarScanner",
    "prepare": "husky install",
    "prettier": "prettier --write 'src/**/*.js'",
    "lint": "eslint .",
    "lint:js": "npx eslint src",
    "lint:js:fix": "npx eslint src --fix",
    "lint:report": "npx eslint src -o ./eslint-report.json --format json"
  },
  "standard-version": {
    "compareUrlFormat": "http://bitbucket.bch.bancodechile.cl:7990/projects/INVA/repos/react-native-minerva/compare/commits?targetBranch=refs%2Ftags%2F{{previousTag}}&sourceBranch=refs%2Ftags%2F{{currentTag}}&targetRepoId=5999",
    "commitUrlFormat": "http://bitbucket.bch.bancodechile.cl:7990/projects/INVA/repos/react-native-minerva/commits/{{hash}}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/github_account/react-native-minerva.git",
    "baseUrl": "https://github.com/github_account/react-native-minerva"
  },
  "keywords": [
    "react-native"
  ],
  "author": {
    "name": "Maria Vargas",
    "email": "mavargasc@bancochile.cl"
  },
  "license": "MIT",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@bch/eslint-config": "git+ssh://git@bitbucket.bch.bancodechile.cl:7999/inva/eslint-config-bch.git#v1.7.0",
    "@bch/prettier-config": "git+ssh://git@bitbucket.bch.bancodechile.cl:7999/inva/prettier-config-bch.git#v1.1.1",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-picker/picker": "2.2.0",
    "@react-navigation/bottom-tabs": "6.0.9",
    "@react-navigation/elements": "^1.2.1",
    "@react-navigation/native": "6.0.2",
    "@react-navigation/native-stack": "6.0.5",
    "@react-navigation/stack": "6.0.6",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^9.1.0",
    "babel-jest": "^27.3.1",
    "bch-mobile-constants": "git+ssh://git@bitbucket.bch.bancodechile.cl:7999/inva/bch-mobile-constants.git#v2.5.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-react-native-a11y": "^3.2.1",
    "eslint-plugin-sonarjs": "^0.11.0",
    "file-name": "^0.1.0",
    "generate-template-files": "^3.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": ">=10",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "4.1.3",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-native": "0.66.4",
    "react-native-gesture-handler": "1.10.3",
    "react-native-linear-gradient": "2.5.6",
    "react-native-reanimated": "2.2.4",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "2.18.1",
    "react-test-renderer": "^17.0.2",
    "replace-in-file": "6.1.0",
    "sonarqube-scanner": "^2.8.1",
    "standard-version": "^9.3.2"
  },
  "dependencies": {
    "bch-assets": "git+ssh://git@bitbucket.bch.bancodechile.cl:7999/pocm/bch-assets.git#v3.22.0",
    "compare-versions": "3.6.0",
    "moment": "2.29.1",
    "native-base": "git+ssh://git@bitbucket.bch.bancodechile.cl:7999/inva/native-base.git#v2.17.0",
    "react-native-animatable": "1.3.3",
    "react-native-collapsible": "git+ssh://git@bitbucket.bch.bancodechile.cl:7999/inva/react-native-collapsible.git#v1.7.0",
    "react-native-dropdownalert": "4.3.0",
    "react-native-fast-image": "8.6.3",
    "react-native-highlight-words": "git+ssh://git@bitbucket.bch.bancodechile.cl:7999/inva/react-native-highlight-words.git#v1.0.3",
    "react-native-indicators": "0.17.0",
    "react-native-material-ripple": "0.9.1",
    "react-native-modal": "13.0.0",
    "react-native-progress": "5.0.0",
    "react-native-snap-carousel": "4.0.0-beta.6",
    "react-native-swipe-list-view": "3.2.9",
    "rn-placeholder": "3.0.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "eslintIgnore": [
    "*.obfuscated.js",
    "jest/*.js"
  ],
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 4,
    "env": {
      "test": {
        "reportPath": "coverage"
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
