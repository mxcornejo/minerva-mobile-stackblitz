{
  "name": "bch-mobile-login",
  "version": "1.3.0",
  "description": "UI login",
  "main": "index.js",
  "scripts": {
    "release": "standard-version",
    "test": "jest --config=jest/jest.config.js --detectOpenHandles",
    "test:watch": "jest --watch --verbose false",
    "sonar:scan": "node sonarScanner",
    "sonar": "npx eslint src -o ./eslint-report.json --format json; npx jest  --detectOpenHandles --config=jest/jest.config.js; node sonarScanner",
    "prepare": "husky install",
    "prettier": "prettier --write 'src/**/*.js'",
    "lint": "eslint .",
    "lint:js": "npx eslint src",
    "lint:js:fix": "npx eslint src --fix",
    "lint:report": "npx eslint src -o ./eslint-report.json --format json"
  },
  "standard-version": {
    "compareUrlFormat": "http://bitbucket.bch.bancodechile.cl:7990/projects/INVA/repos/bch-mobile-login/compare/commits?targetBranch=refs%2Ftags%2F{{previousTag}}&sourceBranch=refs%2Ftags%2F{{currentTag}}&targetRepoId=5999",
    "commitUrlFormat": "http://bitbucket.bch.bancodechile.cl:7990/projects/INVA/repos/bch-mobile-login/commits/{{hash}}"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@bitbucket.bch.bancodechile.cl:7999/inva/bch-mobile-login.git"
  },
  "author": "Maria Vargas",
  "license": "ISC",
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@bch/eslint-config": "git+ssh://git@bitbucket.bch.bancodechile.cl:7999/inva/eslint-config-bch.git#v1.6.0",
    "@bch/prettier-config": "git+ssh://git@bitbucket.bch.bancodechile.cl:7999/inva/prettier-config-bch.git#v1.1.1",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/eslint-config": "^3.0.1",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-picker/picker": "2.2.0",
    "@react-navigation/bottom-tabs": "6.2.0",
    "@react-navigation/drawer": "^6.4.2",
    "@react-navigation/native": "6.0.8",
    "@react-navigation/stack": "6.1.1",
    "@testing-library/react-native": "^8.0.0",
    "babel-jest": "^27.3.1",
    "bch-mobile-constants": "git+ssh://git@bitbucket.bch.bancodechile.cl:7999/inva/bch-mobile-constants.git#v2.1.0",
    "bch-mobile-utils": "git+ssh://git@bitbucket.bch.bancodechile.cl:7999/inva/bch-mobile-utils.git#v1.3.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-react-native-a11y": "^3.2.1",
    "eslint-plugin-sonarjs": "^0.11.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "4.1.3",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-native": "^0.66.3",
    "react-native-gesture-handler": "1.10.3",
    "react-native-linear-gradient": "2.5.6",
    "react-native-minerva": "git+ssh://git@bitbucket.bch.bancodechile.cl:7999/inva/react-native-minerva.git#rn66/master",
    "react-native-reanimated": "2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "2.18.1",
    "react-test-renderer": "^17.0.2",
    "sonarqube-scanner": "^2.8.1",
    "standard": "^16.0.4",
    "standard-version": "^9.3.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "eslintIgnore": [
    "*.obfuscated.js",
    "jest/*.js"
  ],
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 4,
    "env": {
      "test": {
        "reportPath": "coverage"
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
